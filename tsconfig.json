{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",

    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["src/*"],
      "@controllers/*": ["controllers/*"],
      "@libs/*": ["libs/*"],
      "@routes/*": ["routes/*"],
      "@services/*": ["services/*"],
      "@utils/*": ["utils/*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  }
}
